functions: h/1

// this should NOT lead to attacks, as the adversary cannot distinguish a nonce and a hash of a nonce which it does not know
rule Out_Hash:
  [ Fr(~ltk) ]
  -->
  [ Out(diff(h(~ltk),~ltk)) ]


  
end
