theory Blind_Signature
begin

functions: op/2, tc/2, f/2
equations: op( tc(x,z),z ) = x,
           tc(x,f(y, z)) = tc(y,z)
rule R_1:
  let m1 = tc(~x, ~z)
  in
    [ Fr( ~x ), Fr( ~y ), Fr( ~z ) ]
  --[ Pas_1 (m1)]->
    [ St1( m1,~z ) ]
